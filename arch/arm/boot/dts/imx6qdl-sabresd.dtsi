	gpio-keys {
		compatible = "gpio-keys";
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_gpio_keys>;

		power {
			label = "Power Button";
			gpios = <&gpio4 11 1>;
			gpio-key,wakeup;
			linux,code = <KEY_POWER>;
		};

		volume-up {
			label = "Volume Up";
			gpios = <&gpio5 21 1>;
			gpio-key;
			linux,code = <KEY_VOLUMEUP>;
		};
		volume-down {
			label = "Volume Down";
			gpios = <&gpio5 20 1>;
			gpio-key;
			linux,code = <KEY_VOLUMEDOWN>;
		};

		Button_A {
			label = "Button_A";
			gpios = <&gpio5 25 1>;
			gpio-key;
			linux,code = <KEY_J>;   /*A-->J*/
		};

		Button_B {
			label = "Button_B";
			gpios = <&gpio5 24 1>;
			gpio-key;
			linux,code = <KEY_SPACE>;   /*空格*/
		};

		Button_C {							
			label = "Button_C";	
			gpios = <&gpio5 23 1>;	
			gpio-key;				
			linux,code = <KEY_ESC>;   /*ESC*/
		};							

		Button_D {
			label = "Button_D";
			gpios = <&gpio5 22 1>;
			gpio-key;
			linux,code = <KEY_K>;   /*D-->K*/
		};

		Button_Yes {   
			label = "Button_Yes";
			gpios = <&gpio5 19 1>;
			gpio-key;
			//linux,code = <BTN_Y>;//*0X134*//
			linux,code = <KEY_SELECT>;  /*0X161*/
		};

		Button_No {
			label = "Button_No";
			gpios = <&gpio5 18 1>;
			gpio-key;
			linux,code =<KEY_Z>;//*0X102*//
		};

		/*五向键*/
		Button_Up {
			label = "Up Button";
			gpios = <&gpio1 23 1>;
			gpio-key;
			linux,code = <KEY_UP>;
		};

		Button_Down {
			label = "Down Button";
			gpios = <&gpio1 25 1>;
			gpio-key;
			linux,code = <KEY_DOWN>;
		};

		Button_Left {
			label = "Left Button";
			gpios = <&gpio1 31 1>;
			gpio-key;
			linux,code = <KEY_LEFT>;
		};

		Button_Right {
			label = "Right Button";
			gpios = <&gpio1 22 1>;
			gpio-key;
			linux,code = <KEY_RIGHT>;
		};
			
		Button_Center {
			label = "Center Button";
			gpios = <&gpio1 28 1>;
			gpio-key;
			linux,code = <KEY_SELECT>;  /*0X161*/
		};
	};

	joystick {
		compatible = "gpio-joy";
		pinctrl-names = "default";
		debounce-interval-ms = <1>;
		debounce-range = <3>;
		up-linux,code = <KEY_UP>;
		down-linux,code = <KEY_DOWN>;
		left-linux,code = <KEY_LEFT>;
		right-linux,code = <KEY_RIGHT>;
		detect-max = <192>;
		detect-min = <64>;
		detect-up = <25>;
		detect-down = <230>;
		detect-left = <25>;
		detect-right = <230>;
	};

