	kpp {
		compatible = "fsl,gpio-kpp";
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_kpp1>;
		linux,keymap = <
				MATRIX_KEY(0x1, 0x0, KEY_A)
				MATRIX_KEY(0x1, 0x2, KEY_B)
				MATRIX_KEY(0x0, 0x2, KEY_C)
				MATRIX_KEY(0x2, 0x5, KEY_D)
				MATRIX_KEY(0x2, 0x0, KEY_E)
				MATRIX_KEY(0x3, 0x5, KEY_F)
				MATRIX_KEY(0x3, 0x0, KEY_G)
				MATRIX_KEY(0x3, 0x2, KEY_H)
				MATRIX_KEY(0x5, 0x0, KEY_I)
				MATRIX_KEY(0x5, 0x1, KEY_J)
				MATRIX_KEY(0x1, 0x4, KEY_K)
				MATRIX_KEY(0x1, 0x5, KEY_L)
				MATRIX_KEY(0x0, 0x5, KEY_M)
				MATRIX_KEY(0x0, 0x1, KEY_N)
				MATRIX_KEY(0x2, 0x1, KEY_O)
				MATRIX_KEY(0x2, 0x3, KEY_P)
				MATRIX_KEY(0x3, 0x1, KEY_Q)
				MATRIX_KEY(0x5, 0x2, KEY_R)
				MATRIX_KEY(0x4, 0x0, KEY_S)
				MATRIX_KEY(0x5, 0x5, KEY_T)
				MATRIX_KEY(0x1, 0x3, KEY_U)
				MATRIX_KEY(0x0, 0x3, KEY_V)
				MATRIX_KEY(0x0, 0x4, KEY_W)
				MATRIX_KEY(0x2, 0x4, KEY_X)
				MATRIX_KEY(0x5, 0x4, KEY_Y)
				MATRIX_KEY(0x5, 0x3, KEY_Z)
				MATRIX_KEY(0x3, 0x4, KEY_BACK)
				MATRIX_KEY(0x4, 0x1, KEY_ENTER)
		>;
		irq-gpio = <&gpio5 31 1>;
		gpio-row0 = <&gpio6 0 1>;
		gpio-row1 = <&gpio6 1 1>;
		gpio-row2 = <&gpio6 2 1>;
		gpio-row3 = <&gpio6 3 1>;
		gpio-row4 = <&gpio6 4 1>;
		gpio-row5 = <&gpio6 5 1>;
		status = "okay";
	};


	sound {
		compatible = "fsl,imx6ul-evk-wm8960",
			   "fsl,imx-audio-wm8960";
		model = "wm8960-audio";
		cpu-dai = <&ssi2>;
		audio-codec = <&codec>;
		asrc-controller = <&asrc>;
		codec-master;
		/*
                 * hp-det = <hp-det-pin hp-det-polarity>;
		 * hp-det-pin: JD1 JD2  or JD3
		 * hp-det-polarity = 0: hp detect high for headphone
		 * hp-det-polarity = 1: hp detect high for speaker
		 */
		hp-det = <3 0>;
		hp-det-gpios = <&gpio4 10 0>;
		mic-det-gpios = <&gpio4 10 0>;
		audio-routing =
			"Headset Jack", "HP_L",
			"Headset Jack", "HP_R",
			"Ext Spk", "SPK_LP",
			"Ext Spk", "SPK_LN",
			"Ext Spk", "SPK_RP",
			"Ext Spk", "SPK_RN",
			"LINPUT1", "Hp MIC",
			"LINPUT3", "Hp MIC",
			"RINPUT1", "Main MIC",
			"RINPUT2", "Main MIC",
			"Hp MIC", "MICB",
			"Main MIC", "MICB",
			"CPU-Playback", "ASRC-Playback",
			"Playback", "CPU-Playback",
			"ASRC-Capture", "CPU-Capture",
			"CPU-Capture", "Capture";
		mux-int-port = <2>;
		mux-ext-port = <5>;
	};

